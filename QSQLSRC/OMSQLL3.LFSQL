CREATE VIEW OMSQLL3 (
  SQAPPC ,
  SQAPNM ,
  SQPATH )
  AS
  SELECT SQAPPC, SQAPNM, SQPATH FROM OMSQL WHERE SQAPPC = 'DEMO' ;

LABEL ON TABLE OMSQLL3
  IS 'OMS - SQL View' ;

LABEL ON COLUMN OMSQLL3
( SQAPPC TEXT IS 'Application code' ,
  SQAPNM TEXT IS 'Application name' ,
  SQPATH TEXT IS 'Path code' ) ;

